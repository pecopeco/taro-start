{
	"name": "taro-start",
	"version": "1.0.0",
	"private": true,
	"description": "taro-start",
	"templateInfo": {
		"name": "default",
		"typescript": false,
		"css": "stylus"
	},
	"scripts": {
		"start": "set NODE_ENV=production && taro build --type weapp --watch",
		"build:weapp": "taro build --type weapp --watch",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:jd": "taro build --type jd",
		"build:quickapp": "taro build --type quickapp",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:jd": "npm run build:jd -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@tarojs/components": "3.0.22",
		"@tarojs/runtime": "3.0.22",
		"@tarojs/taro": "3.0.22",
		"dayjs": "^1.9.3",
		"flyio": "^0.6.14",
		"pug": "^3.0.0",
		"pug-plain-loader": "^1.0.0",
		"vue": "^3.0.0-beta.15",
		"vuex": "^4.0.0-beta.4"
	},
	"devDependencies": {
		"@types/webpack-env": "^1.13.6",
		"@tarojs/mini-runner": "3.0.22",
		"@babel/core": "^7.8.0",
		"@tarojs/webpack-runner": "3.0.22",
		"babel-preset-taro": "3.0.22",
		"@vue/compiler-sfc": "^3.0.0-beta.15",
		"vue-loader": "^16.0.0-beta.3",
		"eslint-plugin-vue": "^7.0.0-alpha.6",
		"eslint-config-taro": "3.0.22",
		"eslint": "^6.8.0",
		"stylelint": "9.3.0"
	}
}
